{
  "name": "python2uml",
  "displayName": "Python2UML",
  "description": "",
  "version": "0.0.7",
  "publisher": "yojith",
  "repository": {
    "type": "git",
    "url": "https://github.com/yojith/python2uml"
  },
  "engines": {
    "vscode": "^1.108.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "python2uml.generateUMLFromFiles",
        "title": "Generate UML Diagram from Python Files"
      },
      {
        "command": "python2uml.generateUMLFromFolders",
        "title": "Generate UML Diagram from Python Folders"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
